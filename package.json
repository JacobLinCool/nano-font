{
    "name": "nano-font",
    "version": "0.2.0",
    "description": "A Size Limited Font Collection for Serverless Environment. Including 1300+ Google Fonts!",
    "keywords": [
        "tiny",
        "font",
        "collection"
    ],
    "main": "lib/index.js",
    "module": "lib/index.mjs",
    "types": "lib/index.d.ts",
    "files": [
        "lib"
    ],
    "scripts": {
        "gen": "npx ts-node ./src/build.ts",
        "build": "npm run gen && tsup",
        "format": "prettier --write \"src/*.ts\"",
        "docs": "typedoc ./src/ --tsconfig ./tsconfig.json",
        "test": "jest --coverage"
    },
    "packageManager": "pnpm@7.0.0",
    "author": "JacobLinCool <jacoblincool@gmail.com> (https://github.com/JacobLinCool)",
    "license": "MIT",
    "devDependencies": {
        "@trivago/prettier-plugin-sort-imports": "^3.2.0",
        "@types/node": "^17.0.35",
        "@types/node-fetch": "^2.6.1",
        "dotenv": "^16.0.1",
        "fonteditor-core": "^2.1.8",
        "node-fetch": "^2.6.7",
        "ora": "6.1.0",
        "prettier": "^2.6.2",
        "ts-node": "^10.8.0",
        "tsup": "^5.12.8",
        "typedoc": "^0.22.15",
        "typescript": "^4.6.4"
    },
    "bugs": {
        "url": "https://github.com/JacobLinCool/nano-font/issues"
    },
    "homepage": "https://jacoblincool.github.io/nano-font/",
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com/JacobLinCool/nano-font.git"
    }
}
