    "devDependencies": {
        "@types/node": "^16.11.12",
        "@types/node-fetch": "^2.5.12",
        "dotenv": "^10.0.0",
        "fonteditor-core": "^2.1.8",
        "node-fetch": "^2.6.6",
        "prettier": "^2.5.1",
        "ts-node": "^10.4.0",
        "typedoc": "^0.22.10",
        "typescript": "^4.5.2"
    }
}
